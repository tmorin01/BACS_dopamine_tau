anonymize <- function(df, anon_df) {
# merge df and anon_df
dfmerged <- merge(anon_df, df)
# remove original subjects column
dfmerged <- subset(dfmerged, select = -c(subject))
# rename columns
dfmerged <- dfmerged %>%
rename(subject = anon_sub)
return(dfmerged)
}
